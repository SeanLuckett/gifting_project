
#fb-root.fb_reset

%div.row
  %div.span6
    %h3.offset1 Import your Facebook friends
    #friends-list
      %form
        %ul{:data => { :user_id => @user.id}}
        %button.offset3.span2{type: "submit", class: "btn btn-primary"}
          Import friends

=javascript_include_tag "//ajax.googleapis.com/ajax/libs/jquery/1.8.2/jquery.min.js"
:javascript
  window.jQuery || document.write('<script src="js/vendor/jquery-1.8.2.min.js"><\/script>')

