%div.row
  %h2 Import your Facebook friends

#fb-root.fb_reset
= render "fb_graph_api"

%div.row.panel
  %div.four.columns.centered
    #friends-list
      %form
        %ul.two-up{:data => { :user_id => @user.id}}
        %button{type: "submit", class: "form button"}
          Submit



=javascript_include_tag "//ajax.googleapis.com/ajax/libs/jquery/1.8.2/jquery.min.js"
:javascript
  window.jQuery || document.write('<script src="js/vendor/jquery-1.8.2.min.js"><\/script>')

=javascript_include_tag "fb_init"
